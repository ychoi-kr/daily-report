{
  "name": "daily-report",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install",
    "prepare": "husky",
    "openapi:generate": "npx tsx scripts/generate-openapi.ts",
    "api:types": "npx tsx scripts/generate-api-types.ts",
    "api:generate": "npm run openapi:generate && npm run api:types",
    "format:staged": "./format-staged.sh",
    "perf:k6": "k6 run performance-tests/k6/",
    "perf:artillery": "artillery run performance-tests/artillery/",
    "perf:db-seed": "npx tsx performance-tests/utils/seed-large-dataset.ts",
    "perf:db-cleanup": "npx tsx performance-tests/utils/cleanup-test-data.ts",
    "perf:all": "npm run perf:db-seed && npm run perf:k6 performance-tests/k6/comprehensive-test.js && npm run perf:db-cleanup"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
